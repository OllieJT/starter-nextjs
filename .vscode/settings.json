{
	"files.exclude": {
		"**/*-lock.json": true,
		"**/*.map": true,
		"**/.cache": true,
		"**/.DS_Store": true,
		"**/*.Identifier": true,
		"**/node_modules": true,
		"**/.next": true,
		"next-env.d.ts": false,
		"**/.gitattributes": false
	},
	"files.associations": {
		"**/*.env*": "dotenv",
		"**/*.md": "mdx"
		//"*.css": "postcss"
	},
	"files.watcherExclude": {
		"**/node_modules": true,
		"**/.next": true
	},
	"cSpell.words": ["browserconfig", "cpsp", "fontsource", "msapplication"]
}
